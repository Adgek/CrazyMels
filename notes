public double CalulateTaxes(double subTotal)
{
	double taxAmount = subTotal * 1.13;

	//convert taxAmount to only have 2 decimal places
	return taxAmount;
}

public double CalculateTotalCost(double subTotal, double taxes)
{
	return subTotal + taxes;
}

